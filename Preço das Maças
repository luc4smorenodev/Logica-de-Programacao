Var
// Seção de Declarações das variáveis

macas: inteiro
resultado: real
resultadodois: real
valorvariavel: real
Inicio



escreval("Digite Quantas Maças você quer comprar: ")
leia(macas)

se macas >= 12 entao
resultado <- macas * 1.00
escreval("Você vai pagar: ", resultado)

fimse

se macas < 12 entao
resultadodois <- macas * 1.30
escreval( "Você vai pagar: ", resultadodois)

fimse




